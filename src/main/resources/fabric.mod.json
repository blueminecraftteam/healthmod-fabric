{
  "schemaVersion": 1,
  "id": "healthmod",
  "version": "$version",
  "name": "HealthMod",
  "description": "a mod which adds health related items",
  "authors": [
    "Blue Minecraft Team"
  ],
  "contributors": [
    "xf8b",
    "Secnyt",
