{
  "schemaVersion": 1,
  "id": "healthmod",
  "version": "${mod_version}",
  "name": "HealthMod",
  "description": "a mod which adds health related things",
  "authors": [
    "Team Blue"
  ],
  "contributors": [
    "xf8b",
    "Secnyt",
    "AG6",
    "ilja615",
    "ItalianCucumber",
    "Saksham Kumar",
    "HigherLight",
    "TheOnlyTails"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/healthmod-fabric",
    "repo": "https://github.com/teambluemods/healthmod-fabric/",
    "issues": "https://github.com/teambluemods/healthmod-fabric/issues/"
  },
  "license": "LGPL-3.0-or-later",
  "icon": "assets/healthmod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.blueminecraftteam.healthmod.HealthMod::INSTANCE"
    ],
    "client": [
      "io.github.blueminecraftteam.healthmod.client.HealthModClient::INSTANCE"
    ],
    "modmenu": [
      "io.github.blueminecraftteam.healthmod.api.HealthModModMenuApiImpl::INSTANCE"
    ],
    "cardinal-components-entity": [
      "io.github.blueminecraftteam.healthmod.registries.ComponentRegistries::INSTANCE"
    ]
  },
  "mixins": [
    "healthmod.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": ">=${fabric_version}",
    "fabric-language-kotlin": ">=${fabric_kotlin_version}",
    "minecraft": "~${minecraft_version}"
  },
  "suggests": {
    "modmenu": ">=${mod_menu_version}"
  },
  "custom": {
    "cardinal-components": [
      "healthmod:sanitized_wound",
      "healthmod:cleanliness",
      "healthmod:wounded"
    ]
  }
}
